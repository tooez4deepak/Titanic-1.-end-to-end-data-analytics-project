# Round the values in the Age column
df['Age'] = df['Age'].round()

# Define a function to categorize age
def categorize_age(age):
  if age <= 14:
    return 'Children'
  elif 15 < age <= 64:
    return 'Youth&Adults'
  else:
    return 'Seniors'

# Create the 'age_category' column
df ['age_category'] = df ['Age']. apply(categorize_age)


print(df['age_category'].value_counts())
